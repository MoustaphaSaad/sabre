package main

import "A"

const IMAGE_SIZE = 16;
const RESOURCE_NAME = "mostafa";

@compute{x = IMAGE_SIZE, y = IMAGE_SIZE}
@resource { name = RESOURCE_NAME }
func main(@system_thread_id global_index: uvec3, @system_group_id group_index: uvec3) {
	A.image[global_index.xy] = :vec4{group_index.xy: vec2 / IMAGE_SIZE, 1, 1};
}