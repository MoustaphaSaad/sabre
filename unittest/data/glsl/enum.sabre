type Dir enum {
	Up,
	Down = 2 * 5 + 3,
	Left = 123,
	Right,
	Down_Left = .Down + .Left,
}

const koko: Dir = .Left;

if koko == Dir.Left {
	func offset(d: Dir): vec2 {
		if d == .Up {
			return {0, -1};
		} else if d == .Down {
			return {0, 1};
		} else if d == .Left {
			return {-1, 0};
		} else if d == .Right {
			return {1, 0};
		} else {
			return {};
		}
	}
}